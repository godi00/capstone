<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/enrollboard"
        android:layout_width="344dp"
        android:layout_height="521dp"
        android:layout_marginStart="46dp"
        android:layout_marginEnd="46dp"
        android:layout_marginBottom="18dp"
        android:background="@drawable/button_round"
        app:layout_constraintBottom_toTopOf="@+id/enroll"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <TextView
            android:id="@+id/textView17"
            android:layout_width="63dp"
            android:layout_height="40dp"
            android:layout_marginStart="16dp"
            android:background="#D2EDC5"
            android:gravity="center"
            android:text="> 성별"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView13" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="63dp"
            android:layout_height="38dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="40dp"
            android:background="#D2EDC5"
            android:gravity="center"
            android:text="> 이름"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView13" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="63dp"
            android:layout_height="88dp"
            android:layout_marginStart="16dp"
            android:background="#D2EDC5"
            android:gravity="center"
            android:text="> 실종     장소"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView15" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="63dp"
            android:layout_height="91dp"
            android:layout_marginStart="16dp"
            android:background="#D2EDC5"
            android:gravity="center"
            android:text="> 성격/ 특징"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView14" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="63dp"
            android:layout_height="45dp"
            android:layout_marginStart="16dp"
            android:background="#D2EDC5"
            android:gravity="center"
            android:text="> 털색"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView12" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="63dp"
            android:layout_height="40dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="36dp"
            android:background="#D2EDC5"
            android:gravity="center"
            android:text="> 견종"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text_enroll"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="115dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="115dp"
            android:text="실종 등록하기"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.523"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="63dp"
            android:layout_height="130dp"
            android:layout_marginStart="16dp"
            android:background="#D2EDC5"
            android:gravity="center"
            android:text=">실종견 사진"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView16" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/photo"
            android:layout_width="260dp"
            android:layout_height="131dp"
            android:layout_marginEnd="4dp"
            android:layout_marginBottom="16dp"
            android:background="#D1EDE7"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/upload_img"
                android:layout_width="250dp"
                android:layout_height="112dp"
                android:layout_marginStart="17dp"
                android:layout_marginTop="11dp"
                android:layout_marginEnd="17dp"
                android:background="#B3FFFFFF"
                android:text="(최대 5장까지 사진을 업로드해주세요)"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/input"
            android:layout_width="260dp"
            android:layout_height="338dp"
            android:layout_marginEnd="4dp"
            android:background="@color/white"
            app:layout_constraintBottom_toTopOf="@+id/photo"
            app:layout_constraintEnd_toEndOf="parent" >

            <EditText
                android:id="@+id/name_edit"
                android:layout_width="260dp"
                android:layout_height="38dp"
                android:layout_marginTop="40dp"
                android:background="@color/white"
                android:elevation="3dp"
                android:hint="ex)이름"
                android:inputType="textEmailAddress"
                android:padding="10dp"
                android:textColor="@color/black"
                android:textSize="16sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/specify_edit"
                tools:ignore="TouchTargetSizeCheck" />

            <EditText
                android:id="@+id/address_edit"
                android:layout_width="260dp"
                android:layout_height="88dp"
                android:background="@color/white"
                android:elevation="3dp"
                android:hint="ex)서울특별시"
                android:inputType="textEmailAddress"
                android:padding="10dp"
                android:textColor="@color/black"
                android:textSize="15sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/farcolor_edit"
                tools:ignore="TouchTargetSizeCheck" />

            <EditText
                android:id="@+id/personality_edit"
                android:layout_width="260dp"
                android:layout_height="91dp"
                android:background="@color/white"
                android:elevation="3dp"
                android:hint="ex)온순하고 귀엽습니다"
                android:inputType="textEmailAddress"
                android:padding="10dp"
                android:textColor="@color/black"
                android:textSize="15sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/address_edit"
                tools:ignore="TouchTargetSizeCheck" />

            <EditText
                android:id="@+id/farcolor_edit"
                android:layout_width="260dp"
                android:layout_height="45dp"
                android:background="@color/white"
                android:elevation="3dp"
                android:hint="ex)갈색"
                android:inputType="textEmailAddress"
                android:padding="10dp"
                android:textColor="@color/black"
                android:textSize="16sp"
                app:layout_constraintStart_toStartOf="parent"
                tools:ignore="TouchTargetSizeCheck"
                tools:layout_editor_absoluteY="117dp" />

            <EditText
                android:id="@+id/gender_edit"
                android:layout_width="260dp"
                android:layout_height="40dp"
                android:background="@color/white"
                android:elevation="3dp"
                android:hint="ex)수컷"
                android:inputType="textEmailAddress"
                android:padding="10dp"
                android:textColor="@color/black"
                android:textSize="16sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/specify_edit"
                tools:ignore="TouchTargetSizeCheck" />

            <EditText
                android:id="@+id/specify_edit"
                android:layout_width="260dp"
                android:layout_height="40dp"
                android:background="@color/white"
                android:elevation="3dp"
                android:hint="ex)말티즈"
                android:inputType="textEmailAddress"
                android:padding="10dp"
                android:textColor="@color/black"
                android:textSize="16sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="TouchTargetSizeCheck" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/titlebar"
        android:layout_width="413dp"
        android:layout_height="90dp"
        android:background="#94C67E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/backbtn"
            android:layout_width="42dp"
            android:layout_height="42dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="96dp"
            app:layout_constraintEnd_toStartOf="@+id/logo"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/backbutton" />

        <ImageView
            android:id="@+id/logo"
            android:layout_width="104dp"
            android:layout_height="60dp"
            android:layout_marginStart="154dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="155dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/img" />

        <ImageView
            android:id="@+id/profile"
            android:layout_width="70dp"
            android:layout_height="63dp"
            android:layout_marginStart="67dp"
            android:layout_marginTop="19dp"
            android:layout_marginEnd="34dp"
            android:layout_marginBottom="20dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/logo"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.25"
            app:srcCompat="@drawable/profile" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/miss_board"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="9dp"
        android:text="실종 게시판"
        android:textColor="@color/black"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titlebar" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/enroll"
        android:layout_width="136dp"
        android:layout_height="33dp"
        android:layout_marginEnd="23dp"
        android:layout_marginBottom="21dp"
        android:background="@drawable/rect_round"
        android:text="-> 등록하기"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="TouchTargetSizeCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>