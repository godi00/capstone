<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DDDDDD">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/enrollboard"
        android:layout_width="370dp"
        android:layout_height="600dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="1dp"
        android:background="@drawable/button_round"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/find_board">

        <EditText
            android:id="@+id/feature_edit"
            android:layout_width="270dp"
            android:layout_height="68dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="1dp"
            android:background="@color/white"
            android:privateImeOptions="defaultInputmode=korean"
            android:elevation="3dp"
            android:hint="ex) 얼굴에 점이 3개정도 있고 발을 절고 있었어요"
            android:padding="10dp"
            android:textColor="@color/black"
            android:textSize="12sp"
            app:layout_constraintStart_toEndOf="@+id/textView31"
            app:layout_constraintTop_toBottomOf="@+id/kakoid_edit"
            tools:ignore="TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/textView31"
            android:layout_width="63dp"
            android:layout_height="69dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="1dp"
            android:background="#eef5ed"
            android:gravity="center"
            android:text="특징"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView30" />

        <EditText
            android:id="@+id/kakoid_edit"
            android:layout_width="270dp"
            android:layout_height="35dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="1dp"
            android:background="@color/white"
            android:elevation="3dp"
            android:inputType="textEmailAddress"
            android:padding="10dp"
            android:textColor="@color/black"
            android:textSize="15sp"
            app:layout_constraintStart_toEndOf="@+id/textView30"
            app:layout_constraintTop_toBottomOf="@+id/address_edit"
            tools:ignore="TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/textView30"
            android:layout_width="63dp"
            android:layout_height="35dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="1dp"
            android:background="#eef5ed"
            android:gravity="center"
            android:text="카톡ID"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView14" />

        <EditText
            android:id="@+id/date_edit1"
            android:layout_width="270dp"
            android:layout_height="40dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="1dp"
            android:background="@color/white"
            android:privateImeOptions="defaultInput-mode=korean"
            android:elevation="3dp"
            android:hint="ex) 2023년 3월 21일 오후 4시"
            android:padding="10dp"
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@+id/textView27"
            app:layout_constraintTop_toBottomOf="@+id/age_edit1"
            tools:ignore="TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/textView27"
            android:layout_width="63dp"
            android:layout_height="40dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="1dp"
            android:background="#eef5ed"
            android:gravity="center"
            android:text="목격시간"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView26" />

        <EditText
            android:id="@+id/age_edit1"
            android:layout_width="270dp"
            android:layout_height="40dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="1dp"
            android:background="@color/white"
            android:elevation="3dp"
            android:hint="ex) 2"
            android:inputType="number"
            android:padding="10dp"
            android:privateImeOptions="defaultInput-mode=korean"
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@+id/textView26"
            app:layout_constraintTop_toBottomOf="@+id/gender_edit"
            tools:ignore="TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/textView26"
            android:layout_width="63dp"
            android:layout_height="40dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="1dp"
            android:background="#eef5ed"
            android:gravity="center"
            android:text="추정 나이"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView17" />

        <EditText
            android:id="@+id/farcolor_edit1"
            android:layout_width="135dp"
            android:layout_height="45dp"
            android:layout_marginTop="1dp"
            android:background="@color/white"
            android:privateImeOptions="defaultInput-mode=korean"
            android:elevation="3dp"
            android:hint="ex) 검정색"
            android:padding="10dp"
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@+id/farcolor_edit"
            app:layout_constraintTop_toBottomOf="@+id/specify_edit"
            tools:ignore="TouchTargetSizeCheck" />

        <EditText
            android:id="@+id/specify_edit"
            android:layout_width="270dp"
            android:layout_height="40dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="12dp"
            android:background="@color/white"
            android:privateImeOptions="defaultInput-mode=korean"
            android:elevation="3dp"
            android:hint="ex) 말티즈"
            android:padding="10dp"
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@+id/textView13"
            app:layout_constraintTop_toBottomOf="@+id/text_enroll"
            tools:ignore="TouchTargetSizeCheck" />

        <EditText
            android:id="@+id/farcolor_edit"
            android:layout_width="135dp"
            android:layout_height="45dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="1dp"
            android:background="@color/white"
            android:privateImeOptions="defaultInput-mode=korean"
            android:elevation="3dp"
            android:hint="ex) 갈색"
            android:padding="10dp"
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@+id/textView15"
            app:layout_constraintTop_toBottomOf="@+id/specify_edit"
            tools:ignore="TouchTargetSizeCheck" />

        <EditText
            android:id="@+id/gender_edit"
            android:layout_width="270dp"
            android:layout_height="40dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="1dp"
            android:privateImeOptions="defaultInput-mode=korean"
            android:background="@color/white"
            android:elevation="3dp"
            android:hint="ex) 수컷"
            android:padding="10dp"
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintStart_toEndOf="@+id/textView17"
            app:layout_constraintTop_toBottomOf="@+id/farcolor_edit1"
            tools:ignore="TouchTargetSizeCheck" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="63dp"
            android:layout_height="40dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="1dp"
            android:background="#eef5ed"
            android:gravity="center"
            android:text="성별"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView15" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="63dp"
            android:layout_height="50dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="1dp"
            android:background="#eef5ed"
            android:gravity="center"
            android:text="목격 장소"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView27" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="63dp"
            android:layout_height="45dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="1dp"
            android:background="#eef5ed"
            android:gravity="center"
            android:text="모색1,2"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView13" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="63dp"
            android:layout_height="40dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="36dp"
            android:background="#eef5ed"
            android:gravity="center"
            android:text="추정 품종"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text_enroll"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="115dp"
            android:layout_marginTop="4dp"
            android:layout_marginEnd="115dp"
            android:text="목격 등록하기"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.523"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="63dp"
            android:layout_height="130dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="1dp"
            android:background="#eef5ed"
            android:gravity="center"
            android:text="목격사진"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="16sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView31" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/photo"
            android:layout_width="270dp"
            android:layout_height="130dp"
            android:layout_marginTop="1dp"
            android:background="#b4c5b0"
            app:layout_constraintStart_toEndOf="@+id/textView8"
            app:layout_constraintTop_toBottomOf="@+id/feature_edit">

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerView"
                android:layout_width="270dp"
                android:layout_height="130dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ProgressBar
                android:id="@+id/progress_bar1"
                style="?android:attr/progressBarStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="@+id/recyclerView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.672" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <EditText
            android:id="@+id/address_edit"
            android:layout_width="270dp"
            android:layout_height="50dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="1dp"
            android:background="@color/white"
            android:elevation="3dp"
            android:privateImeOptions="defaultInput-mode=korean"
            android:hint="ex) 서울특별시"
            android:padding="10dp"
            android:textColor="@color/black"
            android:textSize="15sp"
            app:layout_constraintStart_toEndOf="@+id/textView14"
            app:layout_constraintTop_toBottomOf="@+id/date_edit1"
            tools:ignore="TouchTargetSizeCheck" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/findimg"
            android:layout_width="136dp"
            android:layout_height="33dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="14dp"
            android:background="@drawable/rect_round"
            android:text="사진 업로드"
            android:textColor="@color/white"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView8" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/enroll"
            android:layout_width="136dp"
            android:layout_height="33dp"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="21dp"
            android:background="@drawable/rect_round"
            android:text="등록하기"
            android:textColor="@color/white"
            android:textSize="16sp"
            android:textStyle="normal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/photo"
            tools:ignore="TouchTargetSizeCheck" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/titlebar"
        android:layout_width="413dp"
        android:layout_height="70sp"
        android:background="#94C67E"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/logo"
            android:layout_width="80sp"
            android:layout_height="48sp"
            android:layout_marginStart="154dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="155dp"
            android:layout_marginBottom="10dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/img" />

        <ImageView
            android:id="@+id/backbtn"
            android:layout_width="42dp"
            android:layout_height="42dp"
            android:layout_marginStart="17dp"
            android:layout_marginTop="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/backbutton1" />

        <ImageView
            android:id="@+id/profile"
            android:layout_width="42sp"
            android:layout_height="42sp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="36dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/profile" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/find_board"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:text="목격 게시판"
        android:textColor="@color/black"
        android:textSize="17sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titlebar" />

</androidx.constraintlayout.widget.ConstraintLayout>